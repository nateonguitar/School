  <DataSets>
    <DataSet Name="NumberOfReservationsPerProperty">
      <Query>
        <DataSourceName>CS3550Titan</DataSourceName>
        <CommandText>SELECT COUNT(*) AS NumReservations, pr.PropertyName
FROM Property pr
	JOIN UnitRate ur
	ON pr.PropertyID = ur.PropertyID
	JOIN Reservation re
	ON ur.UnitRateID = re.UnitRateID
GROUP BY pr.PropertyName
ORDER BY NumReservations DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="NumReservations">
          <DataField>NumReservations</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="PropertyName">
          <DataField>PropertyName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
