  <DataSets>
    <DataSet Name="HowManyOfEachAmenityDoWeHave">
      <Query>
        <DataSourceName>CS35504</DataSourceName>
        <CommandText>SELECT AmenityDescription, COUNT(*) AS NumUnitsThatHaveAmenity
FROM Unit un
	JOIN UnitAmenity ua
	ON un.UnitID = ua.UnitID
	JOIN Amenity am
	ON ua.AmenityID = am.AmenityID
GROUP BY  AmenityDescription
ORDER BY NumUnitsThatHaveAmenity DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="AmenityDescription">
          <DataField>AmenityDescription</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NumUnitsThatHaveAmenity">
          <DataField>NumUnitsThatHaveAmenity</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
