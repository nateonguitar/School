public class SarahsFirst
{
	public static void main(String args[])
	{
		int count;
		
		count = 5;
		
		
		
		System.out.println("hello world!");
		System.out.println("hello world!");
		System.out.println("hello world!");
		System.out.println(count + count);
		System.out.println("hello world!");
		System.out.println("hello world!");
		System.out.println("hello world!");
		
		
		
		
		
		
		
		
		
		
	}
}
